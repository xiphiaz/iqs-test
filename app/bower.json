  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.3.15",
    "angular-animate": "~1.3.15",
    "angular-ui-router": "~0.2.13",
    "bootstrap": "~3.3.4",
    "jquery": "~2.1.3",
    "moment": "~2.9.0",
    "lodash": "~3.6.0",
    "angular-i18n": "~1.3.15",
    "lil-uuid": "~0.1.0",
    "angular-bootstrap": "~0.12.1",
    "angular-sanitize": "~1.3.15",
    "angular-messages": "~1.3.15",
    "angular-ui-utils": "~0.2.2",
    "angular-bootstrap-show-errors": "~2.3.0",
    "ngprogress": "~1.0.7",
    "font-awesome": "~4.3.0",
    "bootswatch-dist": "superhero",
    "highcharts-release": "~4.1.4",
    "highcharts-ng": "~0.0.8"

  },
  "resolutions": {
    "angular": "1.3.15"
  },
  "devDependencies": {
    "angular-mocks": "~1.3.15"
  },
  "overrides": {
